	        <!--<plugin name="kiva_plugin" filename="./build/libkiva_plugin.so">
		     <dist>1.0</dist>
	    	</plugin> -->
		<plugin name="kiva_rotate" filename="./kiva_plugins/build/libkiva_rotate.so" />
